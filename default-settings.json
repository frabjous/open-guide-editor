{
    "accesskeyfile" : "/tmp/oge-accesskeys.json",
    "autoprocess" : {
        "delay": 1000
    },
    "autosave": {
        "interval": 300000,
        "directory": "/tmp"
    },
    "conversions": {
        "md": {
            "epub": {
                "directOutputCommand": "pandoc -f markdown -t epub -o - %rootdocument%",
                "fileOutputCommand": "pandoc -f markdown -t epub -o %converedfile% %rootdocument%"
            }
        }
    },
    "routines": {
        "md": {
            "html": {
                "command": "pandoc -f markdown -t html --embed-resources --standalone %rootdocument% -o %outputfile%"
            },
            "pdf": {
                "command": "pandoc -f markdown -t pdf %rootdocument% %outputfile%"
            }
        }
    },
    "viewer": {
        "height": 500,
        "width": 900
    }
}
